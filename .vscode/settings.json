{
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "ui-monospace, Menlo, Monaco, 'Cascadia Mono', 'Segoe UI Mono', 'Roboto Mono', 'Oxygen Mono', 'Ubuntu Monospace', 'Source Code Pro', 'Fira Mono', 'Droid Sans Mono', 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "on",
  "editor.matchBrackets": "always",
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.useTabStops": true,
  "emmet.triggerExpansionOnTab": true,
  "explorer.openEditors.visible": 0,
  "files.autoSave": "afterDelay",
  "screencastMode.onlyKeyboardShortcuts": true,
  "terminal.integrated.fontSize": 18,
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "Material Theme Darker High Contrast",
  "workbench.colorCustomizations": {
    "[Material Theme Darker High Contrast]": {
      "editor.background": "#0c0c0c",
      "editor.lineHighlightBackground": "#181818",
      "editorLineNumber.foreground": "#808080",
      "editorLineNumber.activeForeground": "#e8e8e8",
      "sideBar.background": "#080808",
      "sideBarSectionHeader.background": "#0c0c0c",
      "statusBar.background": "#0c0c0c",
      "activityBar.background": "#181818",
      "tab.inactiveBackground": "#0c0c0c",
      "tab.activeBackground": "#0c0c0c",
      "editorGroupHeader.tabsBackground": "#0c0c0c",
      "titleBar.activeBackground": "#181818",
      "titleBar.inactiveBackground": "#0c0c0c",
      "panel.background": "#101010",
      "panel.border": "#101010"
    }
  },
  "workbench.fontAliasing": "antialiased",
  "workbench.statusBar.visible": true,
}